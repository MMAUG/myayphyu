             android:keyEdgeFlags="left"/>
        <Key android:codes="4155" android:keyLabel="ျ"/>
        <Key android:codes="4141" android:keyLabel="ိ"/>
        <Key android:codes="4154" android:keyLabel="်"/>
        <Key android:codes="4140" android:keyLabel="ါ"/>
